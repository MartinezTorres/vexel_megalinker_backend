TABLE1:#i32[3] = [5, 6, 7];
TABLE2:#i32[2] = [10, 11];
TABLE_UNUSED:#i32[1] = [99];
GM:#i32;

&reader() -> #i32 {
  GM = GM + 1;
  idx:#i32 = GM < 3 ? GM : 0;
  x:#i32 = TABLE1[idx];
  x + GM
}

&callee(n:#i32) -> #i32 {
  idx:#i32 = n < 2 ? n : 0;
  n + GM + TABLE2[idx]
}

&caller1() -> #i32 {
  reader() + callee(1)
}

&caller2() -> #i32 {
  callee(2)
}

&^entry_add(n:#i32) -> #i32 {
  callee(n)
}

&^main() -> #i32 {
  GM = 0;
  caller1() + caller2()
}
